#!/bin/bash

current_branch="$(git rev-parse --abbrev-ref HEAD)"
if [ $current_branch != $1 ]
then
	git checkout $1
	echo "Changed to branch $1"
fi
message = "$(git log -1 --pretty=%B --oneline)"
url="$(git pull-request -b master -h $1 -m $message)"
git checkout master
git merge $url
git push origin	master
